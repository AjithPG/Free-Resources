<h1>SEO Meta Tags</h1>

<!--Title-->
<title>Title of your Page</title>
 <!--SEO-->
    <meta name="description" content="Description">
    <meta name="keywords" content="Keywords">
    <meta name="author" content="Author Name">
    
    <!-- Open Graph data -->
        <!--Basic Meta Data-->
    <meta property="og:title" content="Title name" />
    <meta property="og:type" content="Type of your object" />
    <meta property="og:image" content="An image url that represent your object" />
    <meta property="og:url" content="Canonical URL of your object" />
        <!--Optional Meta Data-->
    <meta property="og:description" content="One or two Line description of your object" />
    <meta property="og:site_name" content="If your object is part of a larger web site, the name which should be displayed for the overall site. e.g., "IMDb" />
    <meta property="og:audio" content="An URL to the audio file http://sample.com/sound.mp3" />
    <meta property="og:video" content="An URL to the Video file of your object http://sample.com/video.swf" />
    <meta property="og:determiner" content="the" />//The word that appears before this object's title in a sentence. An enum of (a, an, the, "", auto). If auto is chosen, the consumer of your data should chose between "a" or "an". Default is "" (blank).
    <meta property="og:locale" content="en_GB" />//The locale these tags are marked up in. Of the format language_TERRITORY. Default is en_US.
    <meta property="og:locale:alternate" content="fr_FR" />//alternate
    
    <!-- Schema.org markup / microdata -->//These tags will improve the way search engines read and represent your page in SERPs.
    <meta itemprop="name" content="Your Object name">
    <meta itemprop="description" content="a brief description">
    <meta itemprop="image" content="URL of image">

    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary”">
    <meta name="twitter:site" content="http://sample.com/">
    <meta name="twitter:title" content="Title Name">
    <meta name="twitter:description" content="Description">
    <!-- Twitter summary card with large image must be at least 280x150px -->
    <meta name="twitter:image:src" content="http://sample.com/images/twitter-thumbnil.jpg">
    <meta name="twitter:image:alt" content="alternate name">
    <!--Social Media Analytics-->
    <meta name="twitter:site" content="@twitterid">
